	</RDF:Seq>
	<RDF:Seq about="chrome://global/content/customizeToolbar.xul">
		<RDF:li>chrome://ezsidebar/content/buttonsOverlay.xul</RDF:li>
	</RDF:Seq>

</RDF:RDF>
